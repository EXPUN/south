<SCRIPT LANGUAGE="JavaScript">
function open_window(link){ 
    var arg = '\u003cscript\u003elocation.replace("'+link+'")\u003c/script\u003e';
    window.open('javascript:window.name;', arg,'rel:noreferrer');
}
